<div class="contenedor">
    <h1 class="titulo-web my-5"><%= nombrePagina %></h1>
    <% if (meetis.length > 0 ) { %>
    <div class="grid columnas-md-3 ">
        <% meetis.forEach(meeti => { %>
            <%- include('partials/card-meeti.ejs', {meeti : meeti}); %>
        <% }) %>
    </div><!--grid-->
    <% } else { %>
        <p>No hay resultados</p>
    <% } %>
</div>